{
	"cli": {
		"version": ">= 3.15.1",
		"appVersionSource": "remote"
	},
	"build": {
		"base": {
			"autoIncrement": true
		},
		"development": {
			"extends": "base",
			"env": {
				"EAS_ENV": "development"
			},
			"developmentClient": true,
			"ios": {
				"simulator": true
			}
		},
		"production": {
			"extends": "base",
			"channel": "production",
			"env": {
				"EAS_ENV": "production"
			}
		}
	},
	"submit": {
		"production": {
			"ios": {
				"appleId": "hirdman.alex@icloud.com",
				"appleTeamId": "64FFA8NJWP"
			}
		}
	}
}
