{
  "cli": {
    "version": ">= 3.15.1",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "env": {
        "EAS_ENV": "development"
      },
      "channel": "dev",
      "distribution": "internal",
      "developmentClient": true,
      "ios": {
        "simulator": true
      }
    },
    "test": {
      "env": {
        "EAS_ENV": "test"
      },
      "channel": "test",
      "distribution": "internal",
      "developmentClient": true
    },
    "production": {
      "channel": "production",
      "env": {
        "EAS_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "hirdman.alex@icloud.com",
        "appleTeamId": "64FFA8NJWP"
      }
    }
  }
}
