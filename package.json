{
  "name": "fuse_native",
  "version": "0.0.2",
  "scripts": {
    "start": "expo start --dev-client",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "test": "jest --logHeapUsage",
    "test:e2e": " maestro test ./.maestro/sign-up-flow.yaml",
    "lint": "biome check .",
    "format": "biome format --write .",
    "type-check": "npx tsc --noEmit",
    "verify": "bun run biome && bun run type-check",
    "eas:run": "eas build:run -p ios --latest && bun run start",
    "db:dump": "supabase db dump -f supabase/schema.sql --linked",
    "db:push": "supabase db push",
    "db:pull:remote": "supabase db pull",
    "db:pull:local": "supabase db pull --local",
    "db:gen-types:remote": "supabase gen types typescript --linked > src/lib/supabase/database-generated.types.ts",
    "db:gen-types:local": "supabase gen types typescript --local > src/lib/supabase/database-generated.types.ts",
    "snaplet:seed": "npx tsx seed.mts"
  },
